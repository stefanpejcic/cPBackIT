exec 4>&1
wget -P / ftp://$USER:$PASSWD@$HOST/$FOLDER/$FILE; tar xf /$FILE; 
cat /var/cpanel/users/users | while read CPUSER; do 
echo "Now processing ${CPUSER} ..." /scripts/unpkgacct ${CPUSER};

wait
exit 0

echo "All accounts of this backup file have been restored.";
echo "Cleaning up temporary backup files from local server, they are save on backups server";
rm -rf /$FILE; rm -rf /home/cpmove*;
echo "Thanks for choosing us for your needs in Backups, any further help contact info@cpbackit.in or go to cpbackit.in"